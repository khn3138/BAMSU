<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Page1</servlet-name>
		<servlet-class>kr.ac.shinhan.bamsucsp.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Page1</servlet-name>
		<url-pattern>/regit</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Page2</servlet-name>
		<servlet-class>kr.ac.shinhan.bamsucsp.Register2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Page2</servlet-name>
		<url-pattern>/regit2</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Page3</servlet-name>
		<servlet-class>kr.ac.shinhan.bamsucsp.ListView</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Page3</servlet-name>
		<url-pattern>/listview</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Page4</servlet-name>
		<servlet-class>kr.ac.shinhan.bamsucsp.MyDeleteMember</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Page4</servlet-name>
		<url-pattern>/mydelete</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
